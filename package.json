{
  "name": "jpal-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,apps,libs,test,migrations}/**/*.ts\"",
    "format:check": "prettier --check \"{src,apps,libs,test,migrations}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "check": "npm run prepush",
    "prepush:fix": "npm run format && tsc --noEmit && npm run lint",
    "prepush": "npm run format:check && tsc --noEmit && npm run lint:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:pact": "jest --config ./test/jest-pact.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.ts",
    "migration": "npm run build && npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run build && npm run typeorm migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "seed": "npm run seed:run -- -s \"CreateUsers\" && npm run seed:run -- -s \"CreateReviewers\" && npm run seed:run -- -s \"CreateYouth\" && npm run seed:run -- -s \"CreateSurveyTemplates\" && npm run seed:run -- -s \"CreateSurveys\" && npm run seed:run -- -s \"CreateAssignments\" && npm run seed:run -- -s \"CreateSentences\" && npm run seed:run -- -s \"CreateParagraphs\" && npm run seed:run -- -s \"CreateFragments\"",
    "reset-db": "npm run schema:drop && npm run migration && npm run seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/client-sns": "^3.787.0",
    "@godaddy/terminus": "^4.9.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/typeorm": "8.0.0",
    "@pact-foundation/absolute-version": "^0.0.4",
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mailgun-js": "^0.22.12",
    "aws-sdk": "^2.943.0",
    "bcrypt": "^5.0.1",
    "bottleneck": "^2.19.5",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cognito-jwt-token-validator": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "dotenv": "^8.6.0",
    "image-size": "^2.0.2",
    "js-yaml": "^3.14.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^3.0.1",
    "mailgun-js": "^0.22.0",
    "node-stream": "^1.7.0",
    "nodemailer": "^6.7.7",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.0",
    "@pact-foundation/pact": "^9.17.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.2-beta2",
    "lint-staged": "^11.0.1",
    "nestjs-pact": "2.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,migrations}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}